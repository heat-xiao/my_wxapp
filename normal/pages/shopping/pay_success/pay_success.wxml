<view class="page">
  <view class="weui-msg">
    <view class="weui-msg__icon-area">
      <icon type="success" size="93"></icon>
    </view>
    <view class="weui-msg__text-area">
      <view class="weui-msg__title">支付成功</view>
      <view class="weui-msg__desc">卖家将在24小时内发货</view>
    </view>
    <view class="weui-msg__opr-area">
      <view class="weui-btn-area">
        <view catchtap="goToHome">
          <button class="weui-btn" style="background:#158447;" type="primary">再逛逛</button>
        </view>
        <view catchtap="goToOrderList" style="margin-top:30rpx;">
          <button class="weui-btn" type="default">我的订单</button>
        </view>
      </view>
    </view>
  </view>
</view>