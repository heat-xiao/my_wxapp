.coupon_item {
  width: 90%;
  height: 150rpx;
  margin: 30rpx 5% 0 5%;
  background: #fff;
  background: url('data:image/png;base64,') no-repeat;
  background-size: cover;
}

.coupon_money_box {
  color: #e04904;
  width: 42%;
  float: left;
  height: 100%;
  margin: 0;
  border-right: 1px dotted #e5e5e5;
  line-height: 150rpx;
}

.coupon_money_box view {
  float: left;
}

.coupon_money {
  font-weight: bold;
  font-size: 64rpx;
}

.coupon_info_box {
  float: left;
  margin: 0;
  color: #999;
  font-size: 26rpx;
}

.coupon_money_signal {
  font-size: 24rpx;
  margin-left: 25rpx;
}

.coupon_info_rule {
  margin-top: 39rpx;
}

.coupon_money_desc {
  font-size: 26rpx;
  margin-left: 12rpx;
}

.coupon_info_rule, .coupon_info_time {
  margin-left: 28rpx;
}
