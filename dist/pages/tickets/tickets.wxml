<view class="container page-tickets">
  <view class="route flex-column">
    <view>{{setout}} 至 {{destination}}</view>
    <block wx:if="{{duration}}">
      <view class="duration">(约{{duration}})</view>
    </block>
  </view>
  <view class="toggle-date">
    <view class="decrease weui-cell__ft_in-access" bindtap="decrease"></view>
    <view class="cur-date">{{showDate}}</view>
    <view class="increase weui-cell__ft_in-access" bindtap="increase"></view>
  </view>
    <view class="weui-cells ticket-list">
      <navigator url="../tickettype/tickettype?destination={{destination}}&setout={{setout}}&date={{date}}" class="weui-cell ticket-item" hover-class="weui-cell_active">
        <view class="weui-cell__ft left">
          <view class="weui-media-box__title">16:00</view>
        </view>
        <view class="weui-cell__bd center">
          <view class="start">蛇口港</view>
          <view class="end">香港机场</view>
        </view>
        <view class="weui-cell__ft right">
          <view class="stock">剩余123张</view>
          <view class="price">230</view>
        </view>
      </navigator>

      <navigator url="../tickettype/tickettype?destination={{destination}}&setout={{setout}}&date={{date}}" class="weui-cell ticket-item" hover-class="weui-cell_active">
        <view class="weui-cell__ft left">
          <view class="weui-media-box__title">16:00</view>
        </view>
        <view class="weui-cell__bd center">
          <view class="start">蛇口港</view>
          <view class="end">香港机场</view>
        </view>
        <view class="weui-cell__ft right">
          <view class="stock">剩余123张</view>
          <view class="price">230</view>
        </view>
      </navigator>
    </view>
</view>